{
  "name": "tianhe-ai-assistant",
  "displayName": "天和 AI 助手",
  "description": "天和·智开 IDE 的内置 AI 助手，支持千问 Qwen、Claude、GPT 等多种模型",
  "version": "1.0.0",
  "publisher": "tianhe-zhikai",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "AI",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "ai",
    "assistant",
    "qwen",
    "千问",
    "agent",
    "code-generation"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tianhe-ai-assistant.openChat",
        "title": "打开 AI 对话",
        "category": "天和 AI"
      },
      {
        "command": "tianhe-ai-assistant.newTask",
        "title": "新建 AI 任务",
        "category": "天和 AI"
      },
      {
        "command": "tianhe-ai-assistant.clearHistory",
        "title": "清除对话历史",
        "category": "天和 AI"
      }
    ],
    "configuration": {
      "title": "天和 AI 助手",
      "properties": {
        "tianhe-ai.qwen.apiKey": {
          "type": "string",
          "default": "",
          "description": "千问 API Key (从阿里云百炼获取)",
          "order": 0
        },
        "tianhe-ai.qwen.model": {
          "type": "string",
          "default": "qwen-coder-plus",
          "enum": [
            "qwen-turbo",
            "qwen-plus",
            "qwen-max",
            "qwen-coder-plus",
            "qwen-vl-max"
          ],
          "enumDescriptions": [
            "快速响应，适合开发测试（便宜）",
            "通用对话，平衡性能和成本",
            "高质量输出，适合复杂任务",
            "代码生成专用模型（推荐）",
            "支持图片理解的多模态模型"
          ],
          "description": "千问模型选择",
          "order": 1
        },
        "tianhe-ai.claude.apiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic Claude API Key",
          "order": 2
        },
        "tianhe-ai.openai.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key",
          "order": 3
        },
        "tianhe-ai.gemini.apiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API Key",
          "order": 4
        },
        "tianhe-ai.defaultModel": {
          "type": "string",
          "default": "qwen",
          "enum": [
            "qwen",
            "claude",
            "gpt",
            "gemini"
          ],
          "enumDescriptions": [
            "千问 Qwen（推荐，国产模型）",
            "Claude 3.5 Sonnet",
            "GPT-4 Turbo",
            "Gemini Pro"
          ],
          "description": "默认 AI 模型",
          "order": 5
        },
        "tianhe-ai.temperature": {
          "type": "number",
          "default": 0.7,
          "minimum": 0,
          "maximum": 2,
          "description": "模型温度（0-2，值越高越随机）",
          "order": 6
        },
        "tianhe-ai.maxTokens": {
          "type": "number",
          "default": 4096,
          "minimum": 256,
          "maximum": 128000,
          "description": "最大生成 token 数",
          "order": 7
        },
        "tianhe-ai.enableBrowser": {
          "type": "boolean",
          "default": false,
          "description": "启用浏览器自动化功能（需要安装 Puppeteer）",
          "order": 8
        },
        "tianhe-ai.enableCheckpoints": {
          "type": "boolean",
          "default": true,
          "description": "启用 Git Checkpoint 自动快照",
          "order": 9
        }
      }
    },
    "keybindings": [
      {
        "command": "tianhe-ai-assistant.openChat",
        "key": "ctrl+alt+a",
        "mac": "cmd+alt+a"
      },
      {
        "command": "tianhe-ai-assistant.newTask",
        "key": "ctrl+alt+n",
        "mac": "cmd+alt+n"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run bundle",
    "compile": "tsc -p ./",
    "bundle": "node esbuild.js --production",
    "watch": "node esbuild.js --watch",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.22.0",
    "@anthropic-ai/sdk": "^0.51.0",
    "@anthropic-ai/vertex-sdk": "^0.11.3",
    "@aws-sdk/client-bedrock-runtime": "^3.812.0",
